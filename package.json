{
  "name": "vue2-template",
  "description": "vue2 template with webpack5 + Axios + Vuex + Typescript",
  "author": "brodyliao@msn.com",
  "private": true,
  "scripts": {
    "dev": "npm run clear && cross-env RUN_ENV=dev webpack server --config configuration/webpack.config.dev.js",
    "rd": "npm run clear && cross-env RUN_ENV=rdev webpack server --config configuration/webpack.config.dev.js",
    "d": "npm run clear && cross-env RUN_ENV=dev webpack server --config configuration/webpack.config.dev.js",
    "r:dev": "npm run clear && cross-env RUN_ENV=rdev webpack server  --config configuration/webpack.config.dev.js --open --progress",
    "local": "npm run clear && cross-env RUN_ENV=local webpack server  --config configuration/webpack.config.local.js",
    "r:test": "npm run clear && cross-env RUN_ENV=test webpack server  --config configuration/webpack.config.dev.js --open --progress",
    "check": "npm run clear && cross-env RUN_ENV=check webpack server  --config configuration/webpack.config.dev.js --open --progress",
    "r:fudev": "npm run clear && cross-env RUN_ENV=fudev webpack server  --config configuration/webpack.config.dev.js --open --progress",
    "r:futest": "npm run clear && cross-env RUN_ENV=futest webpack server  --config configuration/webpack.config.dev.js --open --progress",
    "deploy": "npm run clear && npm install && cross-env RUN_ENV=prod webpack --config configuration/webpack.config.prod.js",
    "deploy:local": "npm run clear && cross-env RUN_ENV=prod webpack --config configuration/webpack.config.prod.js",
    "build": "cross-env RUN_ENV=local webpack --config configuration/webpack.config.prod.js",
    "start": "npm run clear && npm run dev",
    "test:coverage": "nyc --reporter=text cross-env RUN_ENV=unittest mochapack --watch --webpack-config configuration/webpack.config.test.js --require test/setup.js test/**/*.spec.js",
    "test:watch": "cross-env RUN_ENV=unittest mochapack",
    "test:single": "cross-env RUN_ENV=unittest mochapack --watch --webpack-config configuration/webpack.config.test.js --require test/setup.js",
    "test:all": "cross-env RUN_ENV=unittest mochapack --webpack-config configuration/webpack.config.test.js --require test/setup.js test/**/*.spec.js",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text npm run test-ci",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --ext .js,.ts,.vue ./src",
    "lint:css": "stylelint **/*.{css,sass,scss}",
    "clear": "rimraf dist",
    "fix-memory-limit": "cross-env LIMIT=4096 increase-memory-limit",
    "commit": "cz"
  },
